<Window x:Name="Todo_window" x:Class="ToDo_List.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ToDo_List"
        mc:Ignorable="d"
        
        Title="ToDo List"
        
        Height="650" 
        Width="1000"
        MinHeight="650"
        MinWidth="1000">
    <Grid>
        <Button x:Name="Logoutbtn" Content="Logout" HorizontalAlignment="Left" Height="39" Margin="624,10,0,0" VerticalAlignment="Top" Width="166" Click="Logoutbtn_Click"/>
        <Button x:Name="Loginbtn" Content="Login" HorizontalAlignment="Left" Height="39" Margin="624,10,0,0" VerticalAlignment="Top" Width="166" Click="Loginbtn_Click"/>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>